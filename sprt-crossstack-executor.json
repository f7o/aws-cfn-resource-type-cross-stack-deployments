{
  "typeName": "SPRT::CrossStack::Executor",
  "description": "Resource schema for cross-account and cross-region stack deployments.",
  "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-rpdk.git",
  "additionalProperties": false,
  "properties": {
    "AccountId": {
      "type": "string",
      "pattern": "[0-9]{12}"
    },
    "ExecutorId": {
      "type": "string"
    }
  },
  "handlers": {
    "create": {
      "permissions": [
        "logs:*"
      ]
    },
    "read": {
      "permissions": [
        "logs:*"
      ]
    },
    "update": {
      "permissions": [
        "logs:*"
      ]
    },
    "delete": {
      "permissions": [
        "logs:*"
      ]
    },
    "list": {
      "permissions": [
        "logs:*"
      ]
    }
  },
  "required": [
    "AccountId"
  ],
  "primaryIdentifier": [
    "/properties/ExecutorId"
  ]
}
